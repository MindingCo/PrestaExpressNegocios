extends layouth

append jscss
    
append content   
        blockquote
          h2= user.nom_usu
        .row
          .col.s4
          .col.s4
          .col.s4  
            input.right-align(placeholder='Filtrar asesor' id='in' onkeyup='filtrartablas()')
        
        br
        .divider
        .section
          table.responsive-table#tabla
            thead 
              tr  
                th Id Asesor
                th Nombre
                th Email
                th Telefono
                th Zona
            tbody
              for asesor in asesores
                tr
                  td= asesor.id_ase
                  td= asesor.nom_ase
                  td= asesor.ema_ase
                  td= asesor.tel_ase
                  td= asesor.nom_zon
                
      
    
